{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "module": "None",
    "target": "ESNext",
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "strictNullChecks": false, // temp
    "noImplicitOverride": true,
    // "noPropertyAccessFromIndexSignature": true,
    // "noUncheckedIndexedAccess": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}